<header>
  <%= link_to "Home", root_path %>
  <nav>
    <ul>
      <% if logged_in? %>
        <li><%= link_to "All Users", users_path %></li>
        <li><%= link_to "#{current_user.full_name}", edit_user_path(current_user) %></li>
        <li><%= link_to "Log out", logout_path, method: "delete" %></li>
      <% else %>
        <li><%= link_to "Log in", login_path %></li>
      <% end %>
    </ul>
  </nav>
</header>
